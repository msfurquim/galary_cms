<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(biography) -%>">
  <span class='title'>
    <%= biography. %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.biographies_biography_path(biography),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_biographies_admin_biography_path(biography),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.biographies_admin_biography_path(biography),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => biography.),
        :method => :delete %>
  </span>
</li>
